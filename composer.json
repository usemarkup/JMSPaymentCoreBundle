{
    "name": "jms/payment-core-bundle",
    "type": "symfony-bundle",
    "description": "A unified API for processing payments with Symfony",
    "keywords": ["payment"],
    "homepage": "http://jmspaymentcorebundle.readthedocs.io",
    "license": "Apache2",
    "authors": [
        {
            "name": "Markup",
            "email": "contact@usemarkup.com"
        },
        {
            "name": "Johannes M. Schmitt",
            "email": "schmittjoh@gmail.com"
        },
        {
            "name": "Paulo Rodrigues Pinto",
            "email": "regularjack@gmail.com"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/schmittjoh/JMSPaymentCoreBundle/contributors"
        }
    ],
    "require": {
        "php": ">=7.1",
        "doctrine/orm": "~2.7",
        "symfony/browser-kit": "^3.4|^4",
        "symfony/config": "^3.4|^4",
        "symfony/dependency-injection": "^3.4|^4",
        "symfony/event-dispatcher": "^3.4|^4",
        "symfony/form": "^3.4|^4",
        "symfony/options-resolver": "^3.4|^4",
        "symfony/http-foundation": "^3.4|^4",
        "symfony/http-kernel": "^3.4|^4",
        "symfony/validator": "^3.4|^4",
        "defuse/php-encryption": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8|~5.4",
        "symfony/phpunit-bridge": "~2.7",
        "doctrine/doctrine-bundle": "~1.6",
        "sensio/framework-extra-bundle": "~3.0",
        "symfony/dom-crawler": "^3.4|^4",
        "symfony/doctrine-bridge": "^3.4|^4",
        "symfony/framework-bundle": "^3.4|^4",
        "symfony/routing": "^3.4|^4",
        "symfony/templating": "^3.4|^4",
        "symfony/twig-bundle": "^3.4|^4",
        "symfony/twig-bridge": "^3.4|^4",
        "twig/twig": "~1.0",
        "matthiasnoback/symfony-config-test": "^2.1",
        "phpstan/phpstan-shim": "^0.11"

    },
    "autoload": {
        "psr-0": { "JMS\\Payment\\CoreBundle": "" }
    },
    "suggest": {
        "phpseclib/mcrypt_compat": "for mcrypt support on >7.1"
    },
    "target-dir": "JMS/Payment/CoreBundle"
}
